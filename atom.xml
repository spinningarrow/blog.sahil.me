---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>The Spinning Arrow</title>

	<link href="http://sahil.me/blog/atom.xml" rel="self"/>
	<link href="http://sahil.me/blog/"/>

	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://sahil.me/blog/</id>

	<author>
		<name>Sahil Bajaj</name>
		<email>mail@sahil.me</email>
	</author>

	{% for post in site.posts limit:2 %}
	<entry>
		<title>{{ post.title }}</title>
		<link href="http://sahil.me{{ site.baseurl }}{{ post.url }}/"/>
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>http://sahil.me{{ site.baseurl }}{{ post.id }}</id>
		<content type="html">{{ post.content | xml_escape }}</content>
		<author>
			<name>Sahil</name>
			<uri>sahil.me</uri>
			<email>mail@sahil.me</email>
		</author>
	</entry>
	{% endfor %}

</feed>